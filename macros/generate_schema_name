{% macro generate_schema_name(custom_schema_name, node) -%}
  {%- if target.name == 'prod' and custom_schema_name is not none -%}
    {{ custom_schema_name | trim }}   -- Nur das Schema wie im .yml ohne Prefix!
  {%- else -%}
    {{ target.schema ~ '_' ~ (custom_schema_name if custom_schema_name is not none else '') | trim }} -- z.B. dev_MART_STG_CSV
  {%- endif -%}
{%- endmacro %}
